        div.modal-win__container
            div.modal-win__close
            p.modal-win__heading.
                Записаться на снятие мерок
            form.modal-win__form
                label.sec9__form-label.modal-win__label
                    |Дата
                    input.sec9__form-input.modal-win__input.modal-win__input_date(type="text" name="date" readonly="true")
                label.sec9__form-label.modal-win__label
                    |Время
                    select.sec9__form-input.sec9__form-input_time.modal-win__input(type="" name="time")
                        option.sec9__form-option.
                            10:00-11:00
                        option.sec9__form-option.
                            11:00-12:00
                        option.sec9__form-option.
                            12:00-13:00
                        option.sec9__form-option.
                            13:00-14:00
                        option.sec9__form-option.
                            14:00-15:00
                        option.sec9__form-option.
                            15:00-16:00
                        option.sec9__form-option.
                            16:00-17:00
                        option.sec9__form-option.
                            17:00-18:00
                        option.sec9__form-option.
                            18:00-19:00
                        option.sec9__form-option.
                            19:00-20:00
